apt update -y
apt install php -y 
curl https://raw.githubusercontent.com/prasathmani/tinyfilemanager/master/tinyfilemanager.php --output index.php 
wget https://github.com/playit-cloud/playit-agent/releases/download/v0.15.19/playit-linux-amd64 
chmod +x playit-linux-amd64 
nohup ./playit-linux-amd64 &> /dev/null &
php -S 0.0.0.0:80
